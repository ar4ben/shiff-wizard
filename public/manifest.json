{
  "name": "SNIFF WIZARD",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "all_frames": false
    }
  ],
  "browser_action": {},
  "web_accessible_resources": ["index.html"],
  "permissions": ["debugger", "storage", "activeTab"]
}
